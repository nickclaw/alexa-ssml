{
  "name": "alexa-ssml",
  "version": "1.0.0",
  "description": "JSX syntax for Speech Synthesis Markup Language (SSML)",
  "license": "MIT",
  "contributors": [
    "Cameron Hunter <hello@cameronhunter.co.uk> (http://cameronhunter.co.uk)",
    "Nicholas Clawson <nickclaw@gmail.com> (http://nickclaw.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nickclaw/alexa-ssml"
  },
  "bugs": {
    "url": "http://github.com/nickclaw/alexa-ssml/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "ava",
    "prepublish": "in-publish && npm run test && npm run build || not-in-publish",
    "publish:major": "npm version major && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "postpublish": "git push origin master --tags"
  },
  "main": "build/index.min.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "prop-types": "^0.2.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.4.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "in-publish": "^2.0.0",
    "rollup": "^0.31.2",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-uglify": "^1.0.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "keywords": [
    "alexa",
    "skills",
    "skill",
    "kit",
    "jsx",
    "ssml",
    "speech",
    "markup",
    "echo",
    "ability"
  ]
}
